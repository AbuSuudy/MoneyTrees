  <log4net>
    <!-- Can have mulitple appenders text file, rolling file , console, to database-->
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="C:\Logs\Monzo\Monzo.txt" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maximumFileSize value="10MB" />
      <maxSizeRollBackups value="5" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date{ABSOLUTE} [%logger] [Line: %L] %message%newline%exception" />
      </layout>
    </appender>
    <root>
      <!-- <level value="All"/> <level value="Info"/> Logs in hierachy Debug and All contrains all the info filters as you go down -->
      <level value="DEBUG" />
      <!-- Appends must be added to root to use-->
      <appender-ref ref="RollingFileAppender" />
    </root>
  </log4net>
